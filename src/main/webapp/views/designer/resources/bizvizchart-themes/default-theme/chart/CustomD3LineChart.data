					"JSContent": "var mqtt;\r\nvar reconnectTimeout = 1000;\r\nvar host = \"mqtt-broker.uat-pipeline-system\";\r\n\r\nvar host = \"192.168.1.112\";\r\nvar port = 9001;\r\nvar client = new Paho.MQTT.Client(host, Number(port), \"clientId\");\r\n\r\n\r\nclient.onConnectionLost = onConnectionLost;\r\nclient.onMessageArrived = onMessageArrived;\r\n\r\n\r\nclient.connect({\r\n    useSSL: false,\r\n    onSuccess: onConnect\r\n});\r\n\r\n\r\nfunction onConnect() {\r\n    console.log(\"onConnect\");\r\n    client.subscribe(\"demo_raj\");\r\n    message = new Paho.MQTT.Message(\"Hello\");\r\n    message.destinationName = \"BDB\";\r\n\r\nvar arrayLength = 100;\r\nnewArray = [];\r\n\r\nfor(var i = 0; i < arrayLength; i++) {\r\n  var y = Math.round(Math.random()*10) + 1\r\n  newArray[i] = y\r\n}\r\n\r\nPlotly.plot('graph', [{\r\n  y: newArray,\r\n  mode: 'lines',\r\n  line: {color: '#80CAF6'}\r\n}]);\r\n\r\n\r\n    client.send(message);\r\n}\r\n\r\nfunction onConnectionLost(responseObject) {\r\n    if (responseObject.errorCode !== 0) {\r\n        console.log(\"onConnectionLost:\" + responseObject.errorMessage);\r\n    }\r\n}\r\n\r\nfunction onMessageArrived(message) {\r\n    var d = JSON.parse(message.payloadString);\r\n    draw(d);\r\n}\r\n\r\nfunction draw(d) {\r\n \/\/   console.log(d);\r\n\r\n var y =  d.humidity;\r\n  newArray = newArray.concat(y)\r\n\r\n  var data_update = {\r\n    y: [newArray]\r\n  };\r\n\r\nsetTimeout(function(){\r\n  newArray.splice(0, 1);\r\nconsole.log(newArray.length);\r\n  Plotly.update('graph', data_update)\r\n}, 10);\r\n\r\n}",
